<div class="feedbacks">
    <svg class="svg-icon" id="fdlt" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="40" height="40">
        <path d="M12 8.5L5.5 15l1.5 1.5L12 12l4.5 4.5L18 15z"/>
    </svg>     
    <div class="feeds">
        <% if (locals.allfeeds) { %>
            <% allfeeds.forEach(allfeed => { %>
                <div class="pfeed">
                    <div class="pname">
                        <%= allfeed.Name %>
                    </div>
                    <div class="star-rating">
                        <div class="stars">
                            <% for (let i = 0; i < 5; i++) { %>
                                <% if (i < Math.floor(allfeed.rate)) { %>
                                    <span class="star filled">&#9733;</span>
                                <% } else if (i === Math.floor(allfeed.rate) && allfeed.rate % 1 >= 0.5) { %>
                                    <span class="star half-filled">&#9733;</span>
                                <% } else { %>
                                    <span class="star">&#9733;</span>
                                <% } %>
                            <% } %>
                        </div>
                        <div class="rating-value"><%= allfeed.rate %></div>
                    </div>
                    <div class="pcomment">
                        <h3>Comment</h3>
                        <p><%= allfeed.like %></p>
                        <p><%= allfeed.additional %></p>
                    </div>
                </div>

            <% }); %>
        <% } %>
    </div>
